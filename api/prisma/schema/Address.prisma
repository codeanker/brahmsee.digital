model Address {
  id     Int      @id @default(autoincrement())
  street String
  number String
  zip    String
  city   String
  Ort    Ort[]
  Person Person[]
}

model Ort {
  id            Int             @id @default(autoincrement())
  name          String
  addressId     Int?
  address       Address?        @relation(fields: [addressId], references: [id], onDelete: SetNull)
  Veranstaltung Veranstaltung[]
}
