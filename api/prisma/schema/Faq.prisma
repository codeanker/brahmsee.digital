model FaqCategory {
  id   Int    @id @default(autoincrement())
  name String
  FAQ  Faq[]
}

model Faq {
  id                 Int                  @id @default(autoincrement())
  question           String
  answer             String
  unterveranstaltung Unterveranstaltung[]
  FaqCategory        FaqCategory?         @relation(fields: [faqCategoryId], references: [id])
  faqCategoryId      Int?
}
