enum MahlzeitType {
  FRUEHSTUECK
  MITTAGESSEN
  ABENDESSEN
}

model Mahlzeit {
  id              String        @id @default(uuid(7))
  type            MahlzeitType
  date            DateTime      @db.Date
  veranstaltungId String
  veranstaltung   Veranstaltung @relation(fields: [veranstaltungId], references: [id], onDelete: Cascade)
  anmeldung       Anmeldung[]
}
