model Address {
  id           String   @id @default(uuid(7))
  street       String
  streetNumber String
  zip          String
  city         String
  country      String
  lat          Float?
  lon          Float?
  valid        Boolean  @default(false)
  Ort          Ort[]
  Person       Person[]
}

model Ort {
  id            String          @id @default(uuid(7))
  name          String
  addressId     String?
  address       Address?        @relation(fields: [addressId], references: [id], onDelete: SetNull)
  Veranstaltung Veranstaltung[]
}
