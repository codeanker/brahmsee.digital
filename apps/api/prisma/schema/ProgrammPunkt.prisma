model ProgrammPunkt {
  id          String   @id @default(uuid(7))
  name        String
  description String?
  startingAt  DateTime
  endingAt    DateTime
  location    String
  responsible String

  veranstaltungId String
  veranstaltung   Veranstaltung @relation(fields: [veranstaltungId], references: [id])
}
